    "fabric": "^5.3.0",
    "puppeteer": "^20.3.0",
    "react-bootstrap": "^2.9.2",
    "react-icons": "^4.12.0",
    "sqlite3": "^5.1.6",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@playwright/test": "^1.37.1",
    "@testing-library/react": "^14.2.1",
    "@types/fabric": "^5.3.6",
    "@types/node": "^20.10.5",
